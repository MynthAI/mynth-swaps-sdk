{
  "name": "mynth-swaps-sdk",
  "version": "0.0.0",
  "description": "SDK for swapping tokens via Mynth",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MynthAI/mynth-swaps-sdk.git"
  },
  "license": "MIT",
  "engines": {
    "node": "^18.18.2"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && npx tsc && rm -rf dist/tests",
    "lint": "concurrently \"npx prettier --check .\" \"npx eslint . --max-warnings=0\"",
    "prettier": "npx prettier -w .",
    "test": "npx ava"
  },
  "dependencies": {
    "@lucid-evolution/lucid": "^0.3.19",
    "arktype": "^2.0.0-rc.0",
    "blake2": "^5.0.0",
    "cardano-ts": "^0.3.0",
    "decimal.js": "^10.4.3",
    "es-toolkit": "^1.16.0",
    "ts-handling": "^0.2.2"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/blake2": "^4.0.4",
    "@types/elliptic": "^6.4.18",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "ava": "^6.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "tsx": "^4.18.0"
  },
  "ava": {
    "files": [
      "**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx",
      "--no-warnings"
    ]
  }
}
